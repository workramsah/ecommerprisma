generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Project {
  id       Int    @id @default(autoincrement())
  playerid String @db.Text
  price    String @db.Text
  names    String @db.Text
  whatsapp String @db.Text
}
